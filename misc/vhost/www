server {
  listen 80;
  listen [::]:80;
  server_name www;

  location / {
    return 301 https://$server_name$request_uri;
  }

  # For Let's Encrypt
  location /.well-known/acme-challenge/ {
    root /usr/share/nginx/html;
  }
}

server {
  listen 443 ssl http2;
  listen [::]:443 ssl http2;
  server_name www;

  ssl_certificate /etc/letsencrypt/live/www/fullchain.pem;
  ssl_certificate_key /etc/letsencrypt/live/www/privkey.pem;
  ssl_trusted_certificate /etc/letsencrypt/live/www/fullchain.pem;

  location / {
    proxy_pass http://127.0.0.1:6004;
    proxy_http_version 1.1;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection 'upgrade';
    proxy_set_header Host $host;
    proxy_cache_bypass $http_upgrade;
  }
}
